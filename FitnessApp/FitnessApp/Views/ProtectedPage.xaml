<?xml version="1.0" encoding="utf-8" ?>

<ScrollView
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FitnessApp.ProtectedPage">

  <Label Text="Protected Photos" FontSize="30" HorizontalOptions="Center" FontAttributes="Bold" />     <Grid>         <Grid.ColumnDefinitions>             <ColumnDefinition Width="1*" />             <ColumnDefinition Width="1*" />             <ColumnDefinition Width="1*" />         </Grid.ColumnDefinitions>         <Grid.RowDefinitions>             <RowDefinition Height="1*" />             <RowDefinition Height="1*" />             <RowDefinition Height="1*" />         </Grid.RowDefinitions>         <BoxView Grid.Column="1" Grid.Row="1" BackgroundColor="Black">Image 1</BoxView>         <BoxView Grid.Column="2" Grid.Row="1" BackgroundColor=“Blue”>Image 2</BoxView>         <BoxView Grid.Column="1" Grid.Row="2" BackgroundColor=“Blue”>Image 3</BoxView>         <BoxView Grid.Column="2" Grid.Row="2" BackgroundColor=“White”>Image 3</BoxView>     </Grid>
    
</ScrollView>





    



